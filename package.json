{
    "name": "genblocks",
    "version": "1.0.0",
    "description": "Generate beautiful Gutenberg blocks instantly using AI",
    "author": "bdtanbir",
    "license": "GPL-2.0-or-later",
    "scripts": {
        "start": "wp-scripts start --webpack-src-dir=blocks/src --output-path=blocks/build",
        "build": "wp-scripts build --webpack-src-dir=blocks/src --output-path=blocks/build",
        "admin:dev": "vite --config admin/vite.config.js",
        "admin:build": "vite build --config admin/vite.config.js",
        "admin:preview": "vite preview --config admin/vite.config.js",
        "build:all": "npm run build && npm run admin:build",
        "packages-update": "wp-scripts packages-update",
        "lint:js": "wp-scripts lint-js",
        "lint:css": "wp-scripts lint-style",
        "lint:pkg-json": "wp-scripts lint-pkg-json",
        "test:unit": "wp-scripts test-unit-js",
        "format": "wp-scripts format"
    },
    "devDependencies": {
        "@vitejs/plugin-vue": "^5.0.0",
        "@wordpress/scripts": "^27.0.0",
        "autoprefixer": "^10.4.16",
        "postcss": "^8.4.32",
        "tailwindcss": "^3.4.0",
        "vite": "^5.0.0"
    },
    "dependencies": {
        "@wordpress/api-fetch": "^6.44.0",
        "@wordpress/block-editor": "^12.15.0",
        "@wordpress/blocks": "^12.24.0",
        "@wordpress/components": "^25.13.0",
        "@wordpress/data": "^9.17.0",
        "@wordpress/element": "^5.24.0",
        "@wordpress/i18n": "^4.47.0",
        "@wordpress/plugins": "^6.15.0",
        "echarts": "^5.4.3",
        "pinia": "^2.1.7",
        "vue": "^3.4.0",
        "vue-echarts": "^6.6.8",
        "vue-router": "^4.2.5"
    }
}
