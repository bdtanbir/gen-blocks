{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "genblocks/ai-generator",
    "version": "1.0.0",
    "title": "AI Block Generator",
    "category": "genblocks",
    "icon": "lightbulb",
    "description": "Generate blocks using AI from natural language prompts.",
    "keywords": ["ai", "generate", "prompt", "genblocks", "artificial intelligence"],
    "textdomain": "gen-blocks",
    "attributes": {
        "prompt": {
            "type": "string",
            "default": ""
        }
    },
    "supports": {
        "html": false,
        "reusable": false,
        "multiple": true
    },
    "editorScript": "genblocks-editor",
    "editorStyle": "genblocks-editor-style"
}
